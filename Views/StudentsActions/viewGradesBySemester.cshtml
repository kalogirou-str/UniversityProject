@model List<StudentGradesBySemesterViewModel>

<h2>Grades by Semester</h2>

@foreach (var semester in Model)
{
    <h3>Semester @semester.Semester</h3>

    <table class="table">
        <thead>
            <tr>
                <th>Course Title</th>
                <th>Grade</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var grade in semester.Courses)
            {
                <tr>
                    <td>@grade.CourseTitle</td>
                    <td>@grade.Grade</td>
                </tr>
            }
        </tbody>
    </table>
}